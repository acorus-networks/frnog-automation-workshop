system {
  host-name {{ ansible_host }};
  root-authentication {
    encrypted-password "$1$fv3Ke4LT$10nlsy3SEJy5ainm.kPTd."; ## SECRET-DATA
    ssh-rsa "ssh-rsa  vagrant insecure public key"; ## SECRET-DATA
  }
  login {
    user vagrant {
      uid 2000;
      class super-user;
      authentication {
        ssh-rsa "ssh-rsa  vagrant insecure public key"; ## SECRET-DATA
      }
    }
  }
  services {
    /* Enable SSH */
    ssh {
      /* Root user able to login via ssh */
      root-login allow;
    }
    /* Enable Netconf */
    netconf {
      ssh;
    }
  }
  syslog {
    user * {
      any emergency;
    }
    file messages {
      any notice;
      authorization info;
    }
    file interactive-commands {
      interactive-commands any;
    }
  }
}
interfaces {
  /* Touch with care - Vagrant */
  em0 {
    unit 0 {
      family inet {
        dhcp;
      }
    }
  }
  /* Don't touch - PFE <-> RE */
  em1 {
    unit 0 {
      family inet {
        address 169.254.0.2/24;
      }
    }
  }
}

